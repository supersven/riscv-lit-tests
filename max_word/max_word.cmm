// RUN: %test-ghc %ghc-args -o %S/max_word.a.out %s %S/print_word.c
// RUN: cat %S/max_word.s | FileCheck %s
// RUN: qemu-riscv64 %S/max_word.a.out | grep "result: 18446744073709551615"

// CHECK: .section .text
// CHECK: .balign 8
// CHECK: .globl test
// CHECK: .type test, @function
// CHECK: test:
test {
      // CHECK: li x20, 18446744073709551615
      R1 = 18446744073709551615;
      // CHECK: j StgReturn
      jump StgReturn [R1];
}
