// RUN: %test-ghc %ghc-args -o %S/one.a.out %s %S/print_word.c
// RUN: cat %S/one.s | FileCheck %s
// RUN: qemu-riscv64 %S/one.a.out | grep -e "^result: 1$"

// CHECK: .section .text
// CHECK: .balign 8
// CHECK: .globl one
// CHECK: .type one, @function
// CHECK: one:
one {
      // CHECK: li x20, 1
      R1 = 1;
      // CHECK: j StgReturn
      jump StgReturn [R1];
}
